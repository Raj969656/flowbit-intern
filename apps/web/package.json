{
  "name": "@flowbit/web",
  "private": true,
  "version": "1.0.0",

"scripts": {
  "prebuild": "npm install typescript",
  "build": "cross-env SKIP_PRISMA_GENERATE=true npx tsc -p tsconfig.json",
  "start": "npx prisma generate --schema=../../prisma/schema.prisma && npx prisma migrate deploy --schema=../../prisma/schema.prisma && node dist/index.js",
  "dev": "ts-node-dev --respawn --transpile-only src/index.ts"
},
"prisma": {
  "skipPostinstall": true

},  "dependencies": {
    "chart.js": "^4.4.0",
    "next": "14.2.13",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
     "@prisma/client": "^5.22.0",
      "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  }
}
